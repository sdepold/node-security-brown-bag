{
  "name": "node-security-brown-bag",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "npm run test-nsp && npm run snyk test && npm run test-retire && npm run test-redos",
    "test-nsp": "nsp check --output summary",
    "test-redos": "find . -name '*.js' -not -path './node_modules/*' -exec redos {} \\;",
    "test-retire": "retire",
    "test-snyk": "snyk test"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "mysql": "^2.13.0",
    "sequelize": "3.16",
    "serve-favicon": "~2.3.2",
    "fury-adapter-swagger": "0.9.6"
  },
  "devDependencies": {
    "nsp": "^2.6.2",
    "retire": "^1.2.11",
    "sequelize-cli": "^2.5.1",
    "snyk": "^1.24.6"
  }
}
